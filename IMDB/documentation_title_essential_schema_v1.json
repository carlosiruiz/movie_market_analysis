{
  "$id": "http://schemas.imdb.com/json/dataset/title_essential_schema_v1.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "akas": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "language": {
            "pattern": "^[a-z]{2,3}$",
            "type": "string"
          },
          "region": {
            "pattern": "^[A-Z]{2,4}$",
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": ["title"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "awards": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "awardName": {
            "type": "string"
          },
          "awardNominationId": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "event": {
            "type": "string"
          },
          "winner": {
            "type": "boolean"
          },
          "year": {
            "type": "integer"
          }
        },
        "required": [
          "awardName",
          "awardNominationId",
          "event",
          "winner",
          "year"
        ],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "creditsByCategory": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "category": {
            "type": "string"
          },
          "credits": {
            "items": {
              "properties": {
                "nameId": {
                  "type": "string"
                },
                "roles": {
                  "items": {
                    "type": "string"
                  },
                  "minItems": 1,
                  "type": "array"
                },
                "jobs": {
                  "items": {
                    "type": "string"
                  },
                  "minItems": 1,
                  "type": "array"
                },
                "billing": {
                  "type": "integer"
                },
                "attributes": {
                  "items": {
                    "type": "string"
                  },
                  "minItems": 1,
                  "type": "array"
                }
              },
              "required": ["nameId"],
              "type": "object"
            },
            "minItems": 1,
            "type": "array"
          }
        },
        "required": ["category", "credits"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "certificates": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "attributes": {
            "items": {
              "type": "string"
            },
            "minItems": 1,
            "type": "array"
          },
          "rating": {
            "type": "string"
          },
          "reason": {
            "type": "string"
          },
          "region": {
            "type": "string"
          }
        },
        "required": ["rating", "region"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "companies": {
      "additionalProperties": false,
      "properties": {
        "distribution": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "miscellaneous": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "production": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "specialEffects": {
          "items": {
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        }
      },
      "type": "object"
    },
    "countries": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array"
    },
    "episodeInfo": {
      "additionalProperties": false,
      "properties": {
        "episodeNumber": {
          "type": "integer"
        },
        "seasonNumber": {
          "type": "integer"
        },
        "seriesTitleId": {
          "pattern": "^tt[0-9]{7,}$",
          "type": "string"
        }
      },
      "required": ["seriesTitleId"],
      "type": "object"
    },
    "genres": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array"
    },
    "image": {
      "additionalProperties": false,
      "properties": {
        "height": {
          "minimum": 1,
          "type": "integer"
        },
        "url": {
          "type": "string"
        },
        "width": {
          "minimum": 1,
          "type": "integer"
        }
      },
      "required": ["height", "url", "width"],
      "type": "object"
    },
    "imdbRating": {
      "additionalProperties": false,
      "properties": {
        "rating": {
          "maximum": 10,
          "minimum": 1,
          "type": "number"
        },
        "numberOfVotes": {
          "type": "integer"
        }
      },
      "required": ["rating", "numberOfVotes"],
      "type": "object"
    },
    "imdbUrl": {
      "format": "uri",
      "type": "string"
    },
    "isAdult": {
      "type": "boolean"
    },
    "keywords": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array"
    },
    "languages": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array"
    },
    "locations": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "place": {
            "type": "string"
          },
          "scenes": {
            "items": {
              "type": "string"
            },
            "minItems": 1,
            "type": "array"
          }
        },
        "required": ["place"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "movieConnections": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "text": {
            "type": "string"
          },
          "titleId": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": ["titleId", "type"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "officialSiteLinks": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "linkTitle": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "required": ["url"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "plot": {
      "type": "string"
    },
    "plotShort": {
      "type": "string"
    },
    "plotMedium": {
      "type": "string"
    },
    "plotLong": {
      "type": "string"
    },
    "principalCastMembers": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "roles": {
            "items": {
              "type": "string"
            },
            "minItems": 1,
            "type": "array"
          },
          "nameId": {
            "pattern": "^nm[0-9]{7,}$",
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "billing": {
            "type": "integer"
          }
        },
        "required": ["nameId", "category"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "principalCrewMembers": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "nameId": {
            "pattern": "^nm[0-9]{7,}$",
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "job": {
            "type": "string"
          }
        },
        "required": ["nameId", "category"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "productionStatus": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "date": {
            "format": "date",
            "type": "string"
          },
          "status": {
            "type": "string"
          }
        },
        "required": ["date", "status"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "releaseDates": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "date": {
            "pattern": "^[0-9]{4}(?:-[0-9]{2}(?:-[0-9]{2})?)?$",
            "type": "string"
          },
          "region": {
            "pattern": "^[A-Z]{2,4}$",
            "type": "string"
          }
        },
        "required": ["date"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "remappedTo": {
      "pattern": "^tt[0-9]{7,}$",
      "type": "string"
    },
    "runtimeMinutes": {
      "type": "integer"
    },
    "seriesInfo": {
      "additionalProperties": false,
      "properties": {
        "episodeTitleIds": {
          "items": {
            "pattern": "^tt[0-9]{7,}$",
            "type": "string"
          },
          "minItems": 1,
          "type": "array"
        },
        "endYear": {
          "type": "integer"
        },
        "startYear": {
          "type": "integer"
        }
      },
      "type": "object"
    },
    "taglines": {
      "items": {
        "type": "string"
      },
      "minItems": 1,
      "type": "array"
    },
    "originalTitle": {
      "type": "string"
    },
    "titleDisplay": {
      "items": {
        "additionalProperties": false,
        "properties": {
          "language": {
            "pattern": "^[a-z]{2,3}$",
            "type": "string"
          },
          "region": {
            "pattern": "^[A-Z]{2,4}$",
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": ["title"],
        "type": "object"
      },
      "minItems": 1,
      "type": "array"
    },
    "titleId": {
      "pattern": "^tt[0-9]{7,}$",
      "type": "string"
    },
    "titleType": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    }
  },
  "required": ["originalTitle", "imdbUrl", "titleId", "titleType"],
  "type": "object"
}
